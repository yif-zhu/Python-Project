/*}*/
#body QComboBox QAbstractItemView::item{
    height:30px;
    background-color:white;
    border:1px solid #E6E6E6;
}
#body QComboBox QAbstractItemView::item:selected{
    color:black;
    background-color: #F3F5FA;
}

#body QLineEdit{
    background-color: #FFFFFF;
    border:1px solid #E6E6E6;
    max-width:320px;
    min-width:320px;
    max-height:30px
}
#submit {
    margin-left: 205px;
    margin-right: 205px;
    margin-bottom: 40px
}



